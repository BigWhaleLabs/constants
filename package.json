    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "tsc-watch": "^5.0.3",

{
  "name": "@big-whale-labs/constants",
  "version": "0.0.11",
  "description": "Various constants used by BWL products",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/constants"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false
  },
  "scripts": {
